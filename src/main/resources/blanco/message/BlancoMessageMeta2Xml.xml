<?xml version="1.0" encoding="UTF-8"?>
<!-- blancoCalcParser define file for blancoMessage -->
<blanco version="0.1.0">
  <target name="blancoCalcParser">
    <blancocalcparser name="message">
      <propertyblock name="blancomessage-common" waitY="1">
        <startstring>メッセージ定義・共通</startstring>
        <propertykey name="name" waitX="2">
          <value>メッセージ定義ID</value>
        </propertykey>
        <propertykey name="package" waitX="2">
          <value>パッケージ</value>
        </propertykey>
        <propertykey name="description" waitX="2">
          <value>説明</value>
        </propertykey>
        <propertykey name="suffix" waitX="2">
          <value>サフィックス</value>
        </propertykey>
        <propertykey name="idEmbedding" waitX="2">
          <value>ID埋め込み</value>
        </propertykey>

        <valuemapping>
          <result>true</result>
          <source>○</source>
        </valuemapping>
      </propertyblock>

      <tableblock name="blancomessage-list" waitY="2" titleheight="3" rowname="field">
        <startstring>メッセージ定義・一覧</startstring>

        <tablecolumn name="no">
          <value>No.</value>
        </tablecolumn>
        <tablecolumn name="key">
          <value>キー</value>
        </tablecolumn>
        <tablecolumn name="level">
          <value>レベル</value>
        </tablecolumn>
        <tablecolumn name="value">
          <value>文字列</value>
        </tablecolumn>

      </tableblock>

    </blancocalcparser>
  </target>
</blanco>
